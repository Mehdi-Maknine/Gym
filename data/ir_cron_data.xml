<odoo>
    <record id="ir_cron_membership_expiry_reminder" model="ir.cron">
        <field name="name">Send Membership Expiry Reminders</field>
        <field name="model_id" ref="model_gym_member"/>
        <field name="state">code</field>
        <field name="code">model.send_membership_expiry_reminders()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="active">True</field>
    </record>
</odoo>
